{
  "expo": {
    "name": "myShelter",
    "slug": "myShelter",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0A2732"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "my-shelter",
    "ios": {
      "requireFullScreen": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.emmsdan.shelter-rccg",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.emmsdan.myShelter",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0A2732"
      }
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/emmsdan/TheShelter",
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
        }
      ]
    ]
  }
}
